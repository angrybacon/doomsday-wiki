{
  "name": "doomsday-wiki",
  "private": true,
  "version": "3.0.0",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "build": "concurrently 'pnpm:build:*' --kill-others --names --prefix-colors auto --success first",
    "build:next": "next build",
    "build:scryfall": "pnpm run scryfall",
    "dev": "concurrently 'pnpm:dev:*' --names --prefix-colors auto",
    "dev:next": "next dev",
    "dev:scryfall": "pnpm run scryfall:dev",
    "lint": "pnpm run --aggregate-output '/^lint:[^:]+$/'",
    "lint:code": "eslint",
    "lint:format": "pnpm run --aggregate-output '/^lint:format:[^:]+$/'",
    "lint:format:code": "prettier --check --log-level warn app/ components/ contexts/ hooks/ scryfall/ theme/ tools/",
    "lint:format:puzzles": "prettier --check --log-level warn puzzles/",
    "lint:format:wiki": "prettier --check --log-level warn markdown/",
    "lint:prune": "knip",
    "lint:typings": "tsc",
    "lint:wiki": "markdownlint --config markdownlint.config.js markdown/",
    "scryfall": "node ./scryfall/server.js",
    "scryfall:dev": "DEBUG=1 pnpm run scryfall",
    "start": "next start",
    "test": "jest",
    "test:coverage": "pnpm run test --coverage",
    "test:watch": "pnpm run test --verbose --watchAll"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@fontsource/libre-baskerville": "5.2.5",
    "@korumite/kiwi": "3.4.2",
    "@mdi/js": "7.4.47",
    "@mdi/react": "1.6.1",
    "@mui/material": "6.4.8",
    "@mui/material-nextjs": "6.4.3",
    "async-sema": "3.1.1",
    "mana-font": "1.18.0",
    "mdast-util-find-and-replace": "3.0.2",
    "next": "15.2.3",
    "plaiceholder": "3.0.0",
    "posthog-js": "1.232.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "10.1.0",
    "react-syntax-highlighter": "15.6.1",
    "rehype-slug": "6.0.0",
    "remark-directive": "4.0.0",
    "remark-gfm": "4.0.1",
    "sharp": "0.33.5",
    "simple-icons": "14.11.1",
    "unified": "11.0.5",
    "unist-util-select": "5.1.0",
    "unist-util-visit": "5.0.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@korumite/ts": "1.1.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/jest": "29.5.14",
    "@types/mdast": "4.0.4",
    "@types/node": "22.13.13",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-syntax-highlighter": "15.5.13",
    "concurrently": "9.1.2",
    "eslint": "9.23.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "knip": "5.46.0",
    "markdownlint-cli": "0.44.0",
    "mdast-util-directive": "3.1.0",
    "prettier": "3.5.3",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0"
  }
}

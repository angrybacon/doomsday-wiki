{
  "name": "doomsday-wiki",
  "private": true,
  "version": "3.0.0",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "concurrently 'NEXT_PUBLIC_HASH=$(git rev-parse --short HEAD) next build' 'yarn scryfall' --names 'next,scryfall' --prefix-colors auto --kill-others --success first",
    "dev": "concurrently 'NEXT_PUBLIC_HASH=$(git rev-parse --short HEAD) next dev' 'yarn scryfall:dev' --names 'next,scryfall' --prefix-colors auto",
    "lint": "concurrently 'yarn:lint:*' --names --prefix-colors auto --success all",
    "lint:code": "eslint .",
    "lint:format": "prettier --check markdown/ puzzles/ scryfall/ src/",
    "lint:prune": "ts-prune --error",
    "lint:typings": "tsc",
    "lint:wiki": "markdownlint markdown/",
    "prepare": "husky install",
    "scryfall": "node ./scryfall/server.js",
    "scryfall:dev": "SCRYFALL_DEBUG=1 yarn scryfall",
    "start": "next start",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --verbose --watchAll"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@fontsource/libre-baskerville": "4.5.10",
    "@mdi/js": "7.2.96",
    "@mdi/react": "1.6.1",
    "@mui/material": "5.12.1",
    "@mui/system": "5.12.1",
    "@mui/utils": "5.12.0",
    "@vercel/analytics": "1.0.0",
    "async-sema": "3.1.1",
    "classnames": "2.3.2",
    "gray-matter": "4.0.3",
    "mana-font": "1.15.4",
    "memfs": "3.5.1",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.4.3",
    "react-markdown": "8.0.7",
    "react-syntax-highlighter": "15.5.0",
    "reading-time": "1.5.0",
    "remark-directive": "2.0.1",
    "remark-gfm": "3.0.1",
    "remark-parse": "10.0.1",
    "remark-slug": "7.0.1",
    "remark-toc": "8.0.1",
    "simple-icons": "8.11.0",
    "unified": "10.1.2",
    "unist-util-select": "4.0.3",
    "unist-util-visit": "4.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/mdast": "3.0.11",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@types/react-syntax-highlighter": "15.5.6",
    "@types/unist": "2.0.6",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "concurrently": "8.0.1",
    "eslint": "8.39.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.1",
    "markdownlint-cli": "0.33.0",
    "mdast-util-directive": "2.2.4",
    "msw": "1.2.1",
    "prettier": "2.8.8",
    "ts-prune": "0.10.3",
    "typescript": "5.0.4"
  }
}
